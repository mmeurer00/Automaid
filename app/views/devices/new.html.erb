<h1> New Device </h1>
<%= form_for(@device) do |f| %>
    <%= f.label :user_device_id %>
    <%= f.text_field :user_device_id %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>
    <%= f.label :description %>
    <%= f.text_field :description %><br>
    <%= f.label :brand %>
    <%= f.text_field :brand %><br>
    <%= f.submit 'Create Device' %>
<% end %>
